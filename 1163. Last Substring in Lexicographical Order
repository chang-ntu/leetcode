#题目结果一定是最大字母到结尾的字符串，因此从后向前遍历该字符串
class Solution:
    def lastSubstring(self, s: str) -> str:
        end = len(s)-1
        index = end
        out = s[index]
        for i in range(end,-1,-1):
            if s[i:len(s)] > out:
                out = s[i:len(s)]
        return out
                    
